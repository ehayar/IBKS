@using System.Data
@model DataSet
@{
    Layout = null;
}
<div class=" col-lg-12 col-md-12 col-12 text-center">
    <h1 class="WelcomeTitle">Welcome To IBKS</h1>
</div>


<table cellpadding="0" cellspacing="0">
    <tr style ="width:100%;">
        <th style="padding-left:10px; padding-right:10px; text-align:center;">Employee Name</th>
        <th style="padding-left:10px; padding-right:10px; text-align:center;" >Date Of Birth</th>
        <th style="padding-left:10px; padding-right:10px; text-align:center;" >Sex</th>
        <th style="padding-left:10px; padding-right:10px; text-align:center;" >Supervisor</th>
        <th style="padding-left:10px; padding-right:10px; text-align:center;" >Branch Id</th>
    </tr>
    @foreach (DataRow row in Model.Tables[0].Rows)
    {
        <tr>
            <td style="text-align:center;">@row["EmployeeName"]</td>
            <td style="text-align:center;">@row["EmployeeDOB"]</td>
            <td style="text-align:center;">@row["EmployeeSex"]</td>
            <td style="text-align:center;">@row["SuperId"]</td>
            <td style="text-align:center;">@row["BranchId"]</td>
        </tr>
    }
</table>
